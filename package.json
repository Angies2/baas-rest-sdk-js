{
  "name": "baas-rest-sdk-js",
  "version": "0.0.1",
  "description": "The js sdk for BaaS REST API, which supports browser and nodejs",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "start": "npm run build&&npm run build:web&&npm run doc",
    "doc": "node_modules/.bin/jsdoc --readme README.md dist/APIClient.js -d docs",
    "build": "node ./src/generator.js",
    "build:web": "node_modules/.bin/webpack --config config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmiot-baas/baas-rest-sdk-js.git"
  },
  "keywords": [
    "cmiot",
    "baas",
    "rest",
    "api",
    "js",
    "browser",
    "nodejs"
  ],
  "author": "baas@cmiot.chinamobile.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmiot-baas/baas-rest-sdk-js/issues"
  },
  "homepage": "https://github.com/cmiot-baas/baas-rest-sdk-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "file-loader": "^1.1.6",
    "jsdoc": "^3.5.5",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.2",
    "request": "^2.85.0",
    "swagger-js-codegen": "^1.10.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "uuid": "^3.1.0"
  }
}
